<!--pages/activity/activity.wxml-->
<map
  id="myMap"
  style="width: 100%; height: 450px;"
  latitude="{{latitude}}"
  longitude="{{longitude}}"
  markers="{{markers}}"
  show-location
  bindmarkertap='markertap'
></map>
<view class='score'>
  {{score}}
</view>
<view class='buttons'>
  <button class='finish-button' bindtap='finishtap'>完成</button>
</view>
